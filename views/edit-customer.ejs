    <center>

        <div>
            <form class="form" action="" method="POST">
                <fieldset>
                    <div id=" legend ">
                        <h1>Edit Customer</h1>
                    </div>
                    <div class=" control-group ">


                        <!-- req.body.name -->
                        <label class=" control-label " for="name">Name</label>
                        <div class=" controls ">
                            <input type=" text " id=" name " name="name" placeholder=" " class=" input-xlarge " style=" width: 300px " value=" <%=file.name%>">
                        </div>
                    </div>

                    <div class=" control-group ">
                        <!-- req.body.phone -->
                        <label class=" control-label " for=" phone ">Phone</label>
                        <div class=" controls ">
                            <input type=" text " id="phone" name="phone" placeholder=" " class=" input-xlarge " style=" width: 300px " value="<%=file.phone%>">
                        </div>
                    </div>

                    <div class=" control-group ">
                        <!-- req.body.address -->
                        <label class=" address " for=" address ">Adress</label>
                        <div class=" controls ">
                            <input type=" text " id=" address " name="address" class=" input-xlarge " style=" width:300px " value="<%=file.address%>">
                        </div>
                    </div>

                    <div class=" control-group ">
                        <!-- req.body.email -->
                        <label class=" control-label " for=" email ">Email</label>
                        <div class=" controls ">
                            <input type=" text " id=" email " name="email" placeholder=" " class=" input-xlarge " style=" width: 300px " value="<%=file.email%>">
                        </div>
                    </div>

                    <div class=" control-group ">
                        <!-- req.body.username -->
                        <label class=" username " for=" username ">Username</label>
                        <div class=" controls ">
                            <input type=" text " id=" username " name="username" class=" input-xlarge " style=" width:300px " value="<%=file.username%>">
                        </div>
                    </div>

                    <div class=" control-group ">
                        <!-- req.body.password-->
                        <label class=" control-label " for=" password ">Password</label>
                        <div class=" controls ">
                            <input type="password" id="password" name="password" placeholder=" " class=" input-xlarge " style=" width: 300px " value="<%=file.password%>">
                        </div>
                    </div>

                    <div class=" control-group ">
                        <!-- Button -->
                        <div class=" controls " style=" padding-top: 20px ">
                            <input type="submit" class="btn btn-success" value="Submit"></input>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>


    </center>



